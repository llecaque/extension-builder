<project name="build tao" default="help">
    	<target name="help">
		<echo msg ="Available Command : " />
		<exec executable="vendor/bin/phing" passthru="true">
			<arg value="-l" />
		</exec>
	</target>
	
	
	<target name="prepare">
		<mkdir dir="src" />
		<exec command="pwd && ls" />
	</target>
	
	<target name="test" depends="prepare">
		
		<echo msg="test" />
	</target>

</project>